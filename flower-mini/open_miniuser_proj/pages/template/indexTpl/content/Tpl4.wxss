/*tpl4.wxss*/

#tpl4 .catelisttitle {
    margin-top: 30rpx;
    height: 30rpx;
    line-height: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #f8f8f8;
    text-align: center;
    font-size: 30rpx;
    color: #555555;
}

#tpl4 .catelisttitle .titleline {
    width: 40rpx;
    height: 2rpx;
    background-color: #555;
}

#tpl4 .titletext {
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    margin: 0 20rpx;
    font-size: 30rpx;
    color: #555555;
    background: #f8f8f8;
}

#tpl4 .newlistbox {
    background-color: transparent!important;
    width: 690rpx!important;
    padding: 0 30rpx;
}

#tpl4 .newlistbox .tablistleft {
    width: 230rpx!important;
    height: 230rpx!important;
}

#tpl4 .tablist {
    background-color: #fff;
    padding: 0;
    margin: 0;
    margin-top: 30rpx;
    border: 1rpx solid rgba(0, 0, 0, 0.05);
}

#tpl4 .newlistbox .tablistright {
    flex: 1;
    height: 190rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: column;
    margin: 20rpx;
}

#tpl4 .blanks {
    text-align: center;
    padding-top: 30rpx;
}

#tpl4 .newlistbox .sjimg {
    position: absolute;
    left: 0;
    top: 0;
    width: 68rpx;
    height: 68rpx;
}

#tpl4 .newlistbox .proprice{
    font-size:30rpx;
}

#tpl4 .newlistbox .delprice {
    font-size:22rpx;
}