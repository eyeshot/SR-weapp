/*订单结算*/

page {
    background-color: #f7f7f7;
    padding-bottom: 140rpx;
}

.cotentbox {
    width: 750rpx;
    max-width: 750rpx;
    overflow-x: hidden;
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
}

.toptitle {
    width: 690rpx;
    height: 200rpx;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    position: relative;
}

.centerline {
    position: absolute;
    left: 0;
    right: 0;
    top: 98rpx;
    height: 4rpx;
    width: 690rpx;
    margin: 0 auto;
    background-color: #f35e5e;
}

.contentcoulmebox {
    position: absolute;
    left: 30rpx;
    right: 30rpx;
    top: 0;
    margin: 0 auto;
    z-index: 2;
    width: 690rpx;
    height: 200rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.contentcoulmebox wx-view {
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    border: 1px solid #f35e5e;
    text-align: center;
    color: #f35e5e;
    font-size: 28rpx;
    line-height: 38rpx;
    background-color: #f7f7f7;
    display: flex;
    justify-content: center;
    flex-direction: column;
}

.contentcoulmebox .contentcoulmeactive {
    background-color: #f35e5e;
    color: #fff;
}
























/**/

.centercontent {
    width: 750rpx;
    overflow-x: hidden;
    background-color: #fff;
    font-size: 30rpx;
    color: #333333;
    display: flex;
    justify-content: flex-start;
    flex-direction: column;
}

.centercontent .centercontentcolume {
    height: 90rpx;
    line-height: 90rpx;
    padding: 0 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    border-top: 1px solid #ebebeb;
}

.centercontent .psfs {
    border-top: 0px solid #ebebeb;
}

.centercontent .psfs .ps {
    flex: 1;
    display: flex;
    justify-content: space-around;
    align-items: center;
}

.centercontent .psfs .ps wx-view {
    width: 158rpx;
    height: 58rpx;
    line-height: 58rpx;
    text-align: center;
    font-size: 28rpx;
    border-radius: 6rpx;
}

.centercontent .psfs .ps .psactive {
    border: 1px solid #f35e5e;
    background-color: #ffe8e8;
    color: #f35e5e;
}

.centercontent .psfs .ps wx-view {
    border: 1px solid #ebebeb;
    background-color: #f7f7f7;
    color: #666666;
}

.custom-placeholder-class {
    color: #bdbdbd;
}

.dgrname wx-input {
    flex: 1;
    margin-left: 46rpx;
}

.tellname {
    display: flex;
}

.tellname wx-picker {
    flex: 1;
    margin-left: 26rpx;
    color: #333333;
    font-size: 30rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.picker {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #333;
    font-size: 30rpx;
}

.arrow-right {
    margin-left: 22rpx;
    width: 15rpx;
    height: 15rpx;
    border-top: 4rpx dashed #acacac;
    border-right: 4rpx dashed #acacac;
    transform: rotate(45deg);
}

.centercontenttitle {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    width: 690rpx;
    margin: 0 auto;
    color: #333333;
    font-size: 28rpx;
    background-color: #f7f7f7;
    margin-bottom: 20rpx;
}

.topimageinfo {
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.pronum {
    font-size: 26rpx;
    color: #666;
}

.topimageinfo .leftimag {
    display: flex;
    justify-content: flex-start;
    flex: 1;
    align-items: center;
    /*height: 160rpx;*/
}

.topimageinfo .leftimag wx-image {
    width: 160rpx;
    height: 160rpx;
    background-color: #bdbdbd;
}

.topimageinfo .leftimag .imgname {
    height: 160rpx;
    margin-left: 20rpx;
    flex: 1;
    display: flex;
    justify-content: space-between;
    flex-direction: column;
    text-align: left;
    color: #666666;
    font-size: 28rpx;
}

.topimageinfo .leftimag .imgname wx-text {
    height: 40rpx;
    line-height: 40rpx;
}

.topimageinfo .leftimag .imgname wx-text:nth-of-type(2) {
    color: #666666;
    font-size: 30rpx;
}

.disabledinput {
    flex: 1;
    display: flex;
    justify-content: flex-end;
    align-items: center;
}

.beizhu {
    display: flex;
    justify-content: flex-start;
    align-items: center;
    width: 690rpx;
    height: 98rpx;
    line-height: 98rpx;
    margin-top: 20rpx;
    padding: 0 30rpx;
    background-color: #fff;
}

.beizhu wx-view {
    font-size: 30rpx;
    color: #333333;
}

.beizhu wx-input {
    font-size: 30rpx;
    flex: 1;
    margin-left: 48rpx;
}















