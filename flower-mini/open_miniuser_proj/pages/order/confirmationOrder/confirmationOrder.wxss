/**/

.buy {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    width: 750rpx;
    height: 100rpx;
    overflow: hidden;
    line-height: 100rpx;
    background-color: #fff;
    z-index: 999999;
    font-size: 32rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}

.buy button {
    border-radius: 0px;
    border: 0;
    height: 100rpx;
    color: #fff;
    line-height: 100rpx;
    background: linear-gradient(to right, #ff663d, #ff4e37);
}

.buy button:hover {
    background-color: #ff4a36;
}

.buy .left {
    flex: 1;
    background-color: #ffffff;
    font-size: 32rpx;
    padding-left: 30rpx;
    color: #666666;
    border-top: 1px solid #ebebeb;
    display: flex;
    justify-content: flex-start;
    align-items: center;
}

.buy .left wx-text {
    color: #f35e5e;
}

.buy .left wx-text:nth-of-type(2) {
    font-size: 24rpx;
}

.buy .right {
    width: 220rpx;
    font-size: 32rpx;
    color: #ffffff;
    text-align: center;
}














